<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hyp.spring.dao.UserMapper">
    <select id="getUserInfoById" parameterType="Integer" resultType="com.hyp.spring.bean.UserInfo">
        select id,userCode,userName,birthDate,mobile from bxfw_fieldpersonnel_user where id = #{id}
    </select>
</mapper>